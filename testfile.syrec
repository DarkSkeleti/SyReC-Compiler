module testmodule1 (in a(3), in b(3), out c)
    wire d,e,f
	a <=> b;
	c <=> d;
	a.1 <=> b.2;
	f <=> c;
	a.(1+1):1 <=> b.1:2;
	f <=> b.1:1;
	++=a;
	--=b;
	~=a;
	a^=(b<<1);
	b^=(a>>1)
	
	

module testmodule2 (inout a, in b, out c, out d)
	if c then
		if d then
			a <=> b
		else
			++=b
		fi d
	else
		++=a
	fi c